{
  "name": "@createjs/soundjs",
  "version": "2.0.0",
  "description": "A JavaScript library that provides a simple API and powerful features to make working with audio a breeze. Easily ties in audio file loading to PreloadJS.",
  "keywords": [
    "createjs",
    "soundjs",
    "gskinner",
    "javascript",
    "audio",
    "webaudio",
    "html5",
    "flash"
  ],
  "author": "gskinner.com, inc.",
  "homepage": "https://createjs.com/soundjs",
  "url": "https://npmjs.com/package/soundjs",
  "bugs": "https://github.com/createjs/soundjs/issues",
  "license": "MIT",
  "contributors": [
    "Grant Skinner <grant@gskinner.com> (https://github.com/gskinner)",
    "Lanny McNie <lanny@gskinner.com> (https://github.com/lannymcnie)",
    "Wes Gorgichuk <wes@gskinner.com> (https://github.com/wdamien)",
    "Blair Ludwig <blair@gskinner.com> ()"
  ],
  "main": "dist/sound.common.js",
  "jsnext:main": "dist/sound.es6.js",
  "jsdelivr": "dist/sound.min.js",
  "logo": "icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/createjs/soundjs.git"
  },
  "npmName": "soundjs",
  "npmFileMap": [
    {
      "basePath": "dist",
      "files": [
        "**/*"
      ]
    }
  ],
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "test": "cd node_modules/@createjs/build && npm run test",
    "test:headless": "cd node_modules/@createjs/build && npm run test:headless",
    "dev": "cd node_modules/@createjs/build && npm run dev",
    "build": "cd node_modules/@createjs/build && npm run build",
    "build:next": "cd node_modules/@createjs/build && npm run build:next",
    "plugins": "cd node_modules/@createjs/build && npm run plugins",
    "docs": "cd node_modules/@createjs/build && npm run docs"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-jest": "^22.0.4",
    "jest": "^22.0.4"
  },
  "engine": "node >= 7.10.1"
}
